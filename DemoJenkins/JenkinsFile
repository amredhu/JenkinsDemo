pipeline{
	agent any
		stages{
			stage('BUILD'){
				steps{
					maven(maven : 'maven_1'){
						sh 'mvn clean compile'
					}
				}
			}
			stage('Testing Stage'){
				steps{
					withMaven(maven : 'maven_1'){
						sh 'mvn test'
					}
				}
			}
			stage('Deploy'){
				steps{
					withMaven(maven : 'maven_1'){
						sh 'mvn deploy'
					}
				}
			}
		}
	
}