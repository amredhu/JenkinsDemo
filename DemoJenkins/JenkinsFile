pipeline{
	agent any
		stages{
			stage('BUILD'){
				steps{
						bat 'cd C:\Program Files (x86)\Jenkins\workspace\jenkins-pipeline-example\DemoJenkins'
						bat 'mvn clean install'
				}
			}
			stage('Testing Stage'){
				steps{
						
						bat 'mvn test'
				}
			}
			stage('Deploy'){
				steps{
						bat 'mvn deploy'
				}
			}
		}
	
}