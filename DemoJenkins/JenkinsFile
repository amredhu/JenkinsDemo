pipeline{
	agent any
		stages{
			stage('BUILD'){
				steps{
					dir('DemoJenkins'){
						bat 'mvn clean install'
					}
						
				}
			}
			stage('Testing Stage'){
				steps{
					dir('DemoJenkins'){
						bat 'mvn test'
					}
				}
			}
			stage('Deploy'){
				steps{
					dir('DemoJenkins'){
						bat 'mvn deploy'
					}
				}
			}
		}
	
}